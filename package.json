{
  "name": "ai-thumbnail-generator",
  "version": "1.0.0",
  "description": "AI-powered YouTube thumbnail generator using OpenAI and DeepAI",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "ai",
    "thumbnail",
    "youtube",
    "openai",
    "deepai",
    "image-generation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.104.0",
    "path": "^0.12.7",
    "url-search-params-polyfill": "^8.2.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^9.34.0",
    "nodemon": "^3.0.1"
  }
}
